{
    "name": "endroid/soccer-calendar-bundle",
    "description": "Endroid Soccer Calendar Bundle for Symfony",
    "keywords": ["endroid", "soccer", "calendar", "bundle", "symfony", "php"],
    "homepage": "https://github.com/endroid/soccer-calendar-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "endroid/installer": "^1.2.2",
        "endroid/soccer-calendar": "^2.1.1",
        "symfony/dependency-injection": "^3.4||^4.4||^5.0",
        "symfony/http-foundation": "^3.4||^4.4||^5.0",
        "symfony/twig-bundle": "^3.4||^4.4||^5.0"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities",
        "symfony/security-checker": "Checks your composer.lock for vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SoccerCalendarBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SoccerCalendarBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
